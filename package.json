{
	"name": "trade-journal",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "yarn smui-theme-light && yarn run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"server": "json-server --watch db.json --port 3004"
	},
	"devDependencies": {
		"@material/typography": "^14.0.0",
		"@smui-extra/autocomplete": "^7.0.0-beta.0",
		"@smui/button": "^7.0.0-beta.0",
		"@smui/icon-button": "^7.0.0-beta.0",
		"@smui/textfield": "^7.0.0-beta.0",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/kit": "^1.0.0",
		"@types/json-server": "^0.14.4",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"json-server": "^0.17.1",
		"postcss": "^8.4.20",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"smui-theme": "^7.0.0-beta.0",
		"supabase": "^1.28.0",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"@supabase/supabase-js": "^2.2.3",
		"@tanstack/svelte-query": "^4.22.0",
		"@tanstack/svelte-table": "^8.7.4",
		"axios": "^1.2.2",
		"chart.js": "^4.1.2",
		"date-picker-svelte": "^2.2.5",
		"dayjs": "^1.11.7",
		"svelte-chartjs": "^3.1.0",
		"svelte-table": "^0.5.2",
		"zod": "^3.20.2"
	}
}
